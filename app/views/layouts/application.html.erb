<!DOCTYPE html>
<html>
<head>
  <title>CNA</title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= stylesheet_link_tag "cna" %>
  <%= stylesheet_link_tag "bootstrap" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<script>
 $(document).ready(function() {
		$('.window').click(function() {
		 var id = $(this).attr('id');
		 var status = $(this).attr('class');
		 
		 if (status == 'window') {
		 	$('#' + id).addClass('on');
		 }
		 
		 else {
		 	$('#' + id).removeClass('on');
		 }
		 		 
		})
 });
</script>
<body>
